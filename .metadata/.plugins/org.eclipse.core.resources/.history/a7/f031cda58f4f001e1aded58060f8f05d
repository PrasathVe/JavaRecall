//import java.util.Scanner;

public class PracticeEx {

	

	public static void main(String[] args) {
		String given = "sarvesh";
		String vow = "aeiou";
		int occurance = 0;
		char[] vowChar = vow.toCharArray();
		char[] charArray = given.toCharArray();
		for (int i=0; i<=charArray.length-1;i++) {		
				if (vowChar[0] == charArray[i] || vowChar[1] == charArray[i] || vowChar[2] == charArray[i] || vowChar[3] == charArray[i] || vowChar[4] == charArray[i]) {
					occurance++;
					System.out.println("The Character :"+charArray[i]);
				}
				
				
			
		}
		System.out.println("The Toal Vowels Presented  : " + occurance);
		
		}
		
		
	}

	
	

	
	
	
		
		

		
		
			
		
		
	


