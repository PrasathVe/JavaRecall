package com.java.collection;

import java.util.ArrayList;

import java.util.Arrays;
import java.util.Collections;
import java.util.List;
import java.util.ListIterator;
import java.util.Scanner;

public class PracticeEx {

	

	public static void main(String[] args) {
		String given = "eulogia";
		String vow = "aeiou";
		char[] vowChar = vow.toCharArray();
		char[] charArray = given.toCharArray();
		for (int i=0; i<=charArray.length;i++) {		
				if (vowChar[0] == charArray[i] || vowChar[1] == charArray[i] || vowChar[2] == charArray[i] || vowChar[3] == charArray[i] || vowChar[4] == charArray[i]) {
					System.out.println("The Vowels are : " + charArray[i] + "In the word " + given);
				}
			
		}
		}
		
		
	}

	
	

	
	
	
		
		

		
		
			
		
		
	


