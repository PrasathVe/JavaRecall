	public static void main(String[] args) {
		int arrayStream []= {50, -20, 0, 30, 40, 60, 10};
		
		if (arrayStream.length>=2) {
			if(arrayStream[0]==arrayStream[6]) {
				System.out.println("First and Last Array are Same");
			}else {
				System.out.println("First and Last Array are not Same");
			}
		}
		
		

		
		

	}
